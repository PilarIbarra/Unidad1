Proceso intercambiarvariables
	Definir  a,b,temp como real;
	escribir "dame el valor de a: ";
	leer a;
	escribir "dame el valor de b: ";
	leer b;
	temp<-b;
	b<-a;
	a<-temp;
	escribir "los valores intercambiados son: ";
	escribir "a) ",a;
	escribir "b) ",b;
FinProceso